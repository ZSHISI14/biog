ransform 0.3s;\r\n        &.panelLeft{\r\n            transform: translateX(0);\r\n        }\r\n        &.panelRight{\r\n            transform: translateX(-50%);\r\n        }\r\n        .panel-effect{\r\n            position: relative;\r\n            display: flex;\r\n            justify-content: center;\r\n            align-items: center;\r\n            height: 100%;\r\n            width: 100vw;\r\n            div{\r\n                background-size: contain;\r\n                position: absolute;\r\n            }\r\n            .effect-1{\r\n                background: url(../svg/effect-no-move.svg) 0 0 no-repeat;\r\n                width: 70vw;\r\n                height: 70vw;\r\n            }\r\n            .effect-2{\r\n                background: url(../svg/effect-move1.svg) 0 0 no-repeat;\r\n                width: 60vw;\r\n                height: 60vw;\r\n                animation: rotate 10s linear  infinite reverse;\r\n            }\r\n            .effect-3{\r\n                background: url(../svg/effect-move2.svg) 0 0 no-repeat;\r\n                width: 20vw;\r\n                height: 20vw;\r\n                animation: rotate 8s linear  infinite ;\r\n            }\r\n            .lyric{\r\n                text-align: center;\r\n                bottom: 10%;\r\n                p{\r\n                    font-size: 13px;\r\n                    color:$color1;\r\n                    margin-top: 10px;\r\n                    \r\n                    &.current{\r\n                        color: $color;\r\n                    }\r\n                }\r\n            }\r\n            @keyframes rotate{\r\n                0%{\r\n                    transform: rotate(0)\r\n                }\r\n                100%{\r\n                    transform: rotate(360deg)\r\n                }\r\n            }\r\n        }\r\n        .panel-lyricd{\r\n            width: 100vw;\r\n            text-align: center;\r\n            line-height: 2;\r\n            margin: 30px 0;\r\n            overflow: hidden;\r\n            .lyrics-container{\r\n                transition:all 3s;\r\n            }\r\n            p{\r\n                color: $color1;\r\n                font-size: 14px;\r\n            }\r\n            p.current{\r\n                color: $color;\r\n            }\r\n        }\r\n    }\r\n    .button{\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n        svg{\r\n            width:24px;\r\n            height:24px;\r\n            margin: 0 20px;\r\n            &.btn-more{\r\n                transform: rotate(90deg);\r\n                margin-left:8px;\r\n            }\r\n        }\r\n        \r\n    }\r\n    .area-bar{\r\n        display: flex;\r\n        padding: 0 20px;\r\n        color: $color1;\r\n        align-items: center;\r\n        .time-start{\r\n            order: 1;\r\n            width:40px;\r\n        }\r\n        .timebar{\r\n            order: 2;\r\n            flex:1;\r\n            height:4px;\r\n            border-radius: 2px;\r\n            margin: 0 10px;\r\n            background: $color3;\r\n            .progress{\r\n                position: relative;\r\n                background: $color2;\r\n                height: 100%;\r\n                width:0%;\r\n                border-radius: 2px;\r\n                transition: all .2s;\r\n\r\n                &::after {\r\n                    content:'';\r\n                    position: absolute;\r\n                    display: block;\r\n                    right: -8px;//一定要写这样相对定位后滚动条增长才会相对移动\r\n                    background: url(../svg/progress.svg) 0 0 no-repeat;\r\n                    height: 16px;\r\n                    width: 16px;\r\n                    top:-6px;\r\n                    background-size: contain;\r\n                }\r\n            }\r\n            \r\n        }\r\n        \r\n        .time-end{\r\n            order: 3;\r\n            width:40px;\r\n        }\r\n    }\r\n    .action{\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n        padding: 10px 10px 10px 10px;\r\n        svg{\r\n            width:28px;\r\n            height:28px;\r\n\r\n        &.btn-play-pause{\r\n            width: 50px;\r\n            height: 50px;\r\n        }\r\n        }\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"9154948b98b1eb583f8572b2a7192e5d","cacheData":{"env":{}}}